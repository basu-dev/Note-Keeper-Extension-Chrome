{
    "manifest_version":2,
    "version":"1.0.0",
    "name":"Notes",
    "description":"Keep Your Notes Safe",
    "browser_action":{
        "default_popup":"popup/popup.html"
    },
    "background":{
        
            "scripts":["background.js"],
            "persistance":false
    },
    "permissions":[
        "tabs",
        "storage"
    ]
}